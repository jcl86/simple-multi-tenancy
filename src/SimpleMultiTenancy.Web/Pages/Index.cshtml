@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">@User.Identity?.Name</h1>

</div>

<p>
    Claims:
</p>
<ul>
    @foreach (var claim in @User.Claims)
    {
        <li>
            <strong>
                @claim.Type:

            </strong>
            <span>
                @claim.Value
            </span>
        </li>
    }
</ul>

